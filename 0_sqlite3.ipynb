   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 원하는 파일명 지정 (존재하지 않는 파일인경우 생성됨)\n",
    "con = sqlite3.connect('./database/example.db')\n",
    "\n",
    "# 실제 파일 경로 대신 :memory: 를 넘기면 RAM에 임시로 데이터베이스가 생성됨\n",
    "# con = sqlite3.connect(':memory:')"
