   "source": [
    "# 한번에 여러 SQL문을 실행하고 싶은 경우 executescript 사용하기\n",
    "cur.executescript(\"\"\"\n",
    "    DROP TABLE IF EXISTS teacher;\n",
    "    DROP TABLE IF EXISTS book;\n",
    "\n",
    "    CREATE TABLE teacher(\n",
    "        firstname,\n",
    "        lastname,\n",
    "        age\n",
    "    );\n",
    "\n",
    "    CREATE TABLE book(\n",
    "        title,\n",
    "        author,\n",
    "        published\n",
    "    );\n",
    "\n",
